coroutine.wrap(CGOV_fake_script)()
local function ZCBOL_fake_script() -- Frame.Drag 
	local script = Instance.new('LocalScript', Frame)

	local frame = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local UserInputService = game:GetService("UserInputService")
	local RunService = game:GetService("RunService")
	
	local function lerp(a, b, t)
		return a + (b - a) * t
	end
	
	local function update(input)
		local delta = input.Position - dragStart
		local newPos = UDim2.new(
			startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y
		)
	
		frame:TweenPosition(newPos, Enum.EasingDirection.Out, Enum.EasingStyle.Sine, 0.1, true)
	end
	
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = frame.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if dragging and input == dragInput then
			update(input)
		end
	end)
	
	
end
coroutine.wrap(ZCBOL_fake_script)()
local function PFQNNLL_fake_script() -- BloxxyAnimationsHub.Keybind 
	local script = Instance.new('LocalScript', BloxxyAnimationsHub)

	--[[ idk why did i use chatgpt do do this shit but i did use chatgpt..
	alr     starts
	]]
	local UserInputService = game:GetService("UserInputService")
	
	-- Reference to the ScreenGui
	local screenGui = script.Parent
	
	-- The keys that will be used to toggle the GUI
	local toggleKey1 = Enum.KeyCode.LeftControl
	local toggleKey2 = Enum.KeyCode.RightControl
	
	-- Function to toggle the GUI visibility
	local function toggleGui()
		screenGui.Enabled = not screenGui.Enabled
	end
	
	-- Event listener for key press
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		-- Check if the input is either of the toggle keys and not processed by the game (e.g., chatting)
		if (input.KeyCode == toggleKey1 or input.KeyCode == toggleKey2) and not gameProcessed then
			toggleGui()
		end
	end)
	
end
coroutine.wrap(PFQNNLL_fake_script)()
